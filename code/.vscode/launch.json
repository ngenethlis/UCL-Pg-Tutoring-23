{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C/C++ Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "../ses3/arrays.c",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "miDebuggerPath": "/path/to/your/gdb",
            "miDebuggerArgs": "",
            "logging": {
                "engineLogging": true,
                "trace": true
            },

            "serverStarted": "GDB started on localhost",
            "filterStderr": false,
            "filterStdout": false,
        }
    ]
}
